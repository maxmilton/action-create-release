{
  "name": "@maxmilton/action-create-release",
  "version": "0.0.28",
  "description": "GitHub action to create a new GitHub release in your CI workflow",
  "repository": "maxmilton/action-create-release",
  "author": "Max Milton <max@happyhackers.xyz>",
  "license": "MIT",
  "private": true,
  "main": "index.js",
  "scripts": {
    "__build": "bun build --outdir=. --target=node --format=cjs --minify src/index.ts",
    "build": "esbuild --bundle --outdir=. --platform=node --minify src/index.ts",
    "lint": "pnpm run lint:js && pnpm run lint:ts",
    "lint:js": "eslint --ignore-path .gitignore --ext .cjs,.js,.mjs,.ts .",
    "lint:ts": "tsc --noEmit",
    "test": "bun test"
  },
  "overrides": {
    "node-fetch": "./src/node-fetch"
  },
  "dependencies": {
    "@actions/core": "1.10.1",
    "@actions/github": "6.0.0"
  },
  "devDependencies": {
    "@types/node": "20.10.7",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "bun-types": "1.0.15",
    "esbuild": "0.19.11",
    "eslint": "8.56.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-unicorn": "50.0.1",
    "prettier": "3.1.1",
    "typescript": "5.3.3"
  }
}
